# Files on the hurd boot floppy root disk

mkdir bin
mkdir boot
mkdir dev
mkdir etc
mkdir hurd
mkdir lib
mkdir sbin
mkdir servers
mkdir tmp
mkdir mnt

copy etc/protocols
copy etc/services

objcopy bin/bash
objcopy bin/cat
objcopy bin/chmod
objcopy bin/chown
objcopy bin/cp
objcopy bin/dd
objcopy bin/ed
objcopy bin/devprobe
objcopy bin/fsysopts
objcopy bin/ftp
